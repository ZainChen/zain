  "scripts": {
    "start": "node scripts/start.js",
    "start-all": "cross-env NODE_ENV=development node --trace-warnings ./node_modules/webpack/bin/webpack --config ./config/webpack.config.electron.js --colors && cross-env NODE_ENV=development node --trace-warnings ./node_modules/webpack/bin/webpack --config ./config/webpack.config.renderer.js --colors && electron  . --env dev --entry main",
    "start-main": "cross-env NODE_ENV=development node --trace-warnings ./node_modules/webpack/bin/webpack --config ./config/webpack.config.electron.js --colors && electron  . --env dev --entry main",
    "build": "node --max_old_space_size=4096 scripts/build.js",
    "build-main": "cross-env NODE_ENV=production node --trace-warnings ./node_modules/webpack/bin/webpack --config ./config/webpack.config.electron.js --progress --profile --colors",
    "test": "node scripts/test.js --env=node",
    "electron-start": "electron . --env build --entry main",
    "electron-build-start": "cross-env NODE_ENV=production node --trace-warnings ./node_modules/webpack/bin/webpack --config ./config/webpack.config.electron.js --progress --profile --colors  && electron  . --env build --entry main",
    "pack": "electron-builder build",
    "mkinst": "electron-builder --pd pack/win-unpacked",
    "mkinst32": "electron-builder --pd pack/win-ia32-unpacked",
    "packall": "node scripts/build.js && npm run build-main && npm run pack",
    "packall-dev": "node scripts/build.js && npm run start-main && npm run pack",
    "release": "npm uninstall oae-zh && npm uninstall oss-zh && npm uninstall orz-zh && npm run packall",
    "get-pkg": "npm i oae-zh && npm i oss-zh && npm i octopus-atom-browser"
  },
  "build": {
    "productName": "Octopus",
    "appId": "com.skieer.octopus",
    "copyright": "Copyright © 2020 Skieer",
    "directories": {
      "app": "app",
      "output": "pack"
    },
    "win": {
      "icon": "public/app.ico",
      "publisherName": "Skieer",
      "verifyUpdateCodeSignature": false,
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "allowToChangeInstallationDirectory": true,
      "allowElevation": false,
      "shortcutName": "八爪鱼采集器 8",
      "installerLanguages": [
        "en-US"
      ]
    },
    "publish": [
      {
        "provider": "generic",
        "url": "https://www.bazhuayu.com/"
      }
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom"
    ],
    "testMatch": [
      "**/__tests__/**/*.{js,jsx,ts,tsx}",
      "**/?(*.)(spec|test).{js,jsx,ts,tsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|ts|tsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|ts|tsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|ts|tsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "web.ts",
      "ts",
      "web.tsx",
      "tsx",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "watchPlugins": [
      "<rootDir>/node_modules/jest-watch-typeahead/filename.js",
      "<rootDir>/node_modules/jest-watch-typeahead/testname.js"
    ]
  },
  "babel": {
    "presets": [
      [
        "react-app",
        {
          "flow": false,
          "typescript": true,
          "useEsMoudle": true
        }
      ]
    ],
    "plugins": [
      [
        "import",
        {
          "libraryName": "antd",
          "libraryDirectory": "lib",
          "style": true
        },
        "ant"
      ]
    ]
  }
}
